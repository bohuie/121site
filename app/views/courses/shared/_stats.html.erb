<% #@course.labs.each do |lab| 
%>
<div class = "row" style = "padding: 5px; margin-bottom: 5px;">	
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<p> Overall </p>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<p> <%= stats.name %> </p>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<% unless @course.students.count == 0 %>
			<p><%= (@course.practices.includes(:question).where('questions.topic_id = ?', stats.id).references(:questions).count.to_f/@course.students.count).round(1) %></p>
		<% else %>
			<p> ~ N/A ~ </p>
		<% end %>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<% unless @course.students.count == 0 %>
			<p><%= (@course.practices.includes(:question).where('practices.correct = ? AND questions.topic_id = ?', true, stats.id).references(:question).count.to_f/@course.students.count).round(1) %></p>
		<% else %>
			<p>  ~ N/A ~ </p>
		<% end %>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<% if !(@course.practices.includes(:question).where('questions.topic_id = ?', stats.id).references(:questions).count == 0) %>
			<p> <%= (@course.practices.includes(:question).where('practices.correct = ? AND questions.topic_id = ? ', true, stats.id).references(:question).count*100/@course.practices.includes(:question).where('questions.topic_id = ?', stats.id).references(:questions).count*100)/100 %>%</p>
		<% else %>
			<p>  ~ N/A ~ </p>
		<% end %>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<% if !(@course.practices.includes(:question).where('questions.topic_id = ?', stats.id).references(:questions).count == 0) %>
			<p> <%= (@course.practices.includes(:question).where('questions.topic_id = ?', stats.id).references(:questions).sum(:totaltime)*100/@course.practices.includes(:question).where('questions.topic_id = ?', stats.id).references(:questions).count*100)/10000 %> seconds</p>
		<% else %>
			<p> ~ N/A ~ </p>
		<% end %>
	</div>
</div>
<% #end 
%>