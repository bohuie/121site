
<div class = "row" style = "padding: 5px; margin-bottom: 5px;">
	<div class = "col-md-3" style = "border-bottom: 1px solid black;">
		<p> <%= studentcreations.title %> - <%= studentcreations.year %></p>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<p> <%= studentcreations.questions_created_in.where(user_id: @user.id).count %> </p>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<p> <%= studentcreations.questions_created_in.where(user_id: @user.id, submitted: true).count%> </p>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<p> <%= studentcreations.questions_created_in.where(user_id: @user.id, submitted: true, grade: "Correct").count%> </p>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<% if !(studentcreations.questions_created_in.where(user_id: @user.id, submitted: true).count == 0) %>
			<p> <%= studentcreations.questions_created_in.where(user_id: @user.id, submitted: true, grade: "Correct").count*100/studentcreations.questions_created_in.where(user_id: @user.id, submitted: true).count*100/100%>% </p>
		<% else %>
			<p> ~ N/A ~ </p>
		<% end %>
	</div>
</div>