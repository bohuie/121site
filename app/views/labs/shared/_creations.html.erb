<div class = "row" style = "padding: 5px; margin-bottom: 5px;">
	<div class = "col-md-3" style = "border-bottom: 1px solid black;">
		<p> ? </p>
	</div>
	<div class = "col-md-3" style = "border-bottom: 1px solid black;">
		<p> <%= creations.name %> </p>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<% unless @lab.students.count == 0 %>
			<p><%= (creations.questions.count/@lab.students.count.to_f).round(1) %></p>
		<% else %>
			<p>~ N/A ~ </p>
		<% end %>
	</div>
	<div class = "col-md-2" style = "border-bottom: 1px solid black;">
		<% unless @lab.students.count == 0 %>
			<p><%= (creations.questions.where(submitted: true).count/@lab.students.count.to_f).round(1) %></p>
		<% else %>
			<p>~ N/A ~ </p>
		<% end %>
	</div>
</div>